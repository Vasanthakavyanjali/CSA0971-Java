import java.util.*;
class Perfect
{
	public static void main(String arg[]){
		int n,i,sum=0,m;
		Scanner sc=new Scanner(System.in);
		System.out.println("enter n value:");
		n=sc.nextInt();
		m=n;
		for(i=1;i<n;i++){
			if(n%i==0){
				sum+=i;
			}
		}
		if(sum==m){
			System.out.println("is a perfect number");
		}
		else{
			System.out.println("is not a perfect number");
		}
	}
}
